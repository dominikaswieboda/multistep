<form class="multistep-form" id="get-info46c" action="/multistep/cel-umowy.html">
    <!--  Wyskakuje wtedy kiedy naciśnieto Spółka Akcyjna lub Spółka z ograniczoną odpowiedzialnością -->
    <fieldset class="multistep-form__section">
        <legend>Dane firmy:</legend>
        <div class="multistep-form__desc-heading">
            <p>Tu wpisujemy informacje dotyczące <strong>firmy, która otrzymuje</strong> informacje</p>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="inputNameCompany911c" class="star-red">Nazwa firmy</label>
                <input type="text" class="form-control" id="inputNameCompany911c" name="inputNameCompany911c" placeholder="Nazwa firmy">
            </div>
            <div class="form-group col-md-12">
                <label for="exampleFormControlSelect911c" class="star-red">Sąd rejestrowy</label>
                <select class="form-control" id="exampleFormControlSelect911c">
                    <option>Sąd rejestrowy 1</option>
                    <option>Sąd rejestrowy 2</option>
                    <option>Sąd rejestrowy 3</option>
                </select>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-sm-4">
                <label for="inputKrs911c">Nr KRS</label>
                <input type="text" class="form-control" id="inputKrs46c" name="inputKrs911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputRegon911c">Regon</label>
                <input type="text" class="form-control" id="inputRegon911c" name="inputRegon911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputNip911c">Nip</label>
                <input type="text" class="form-control" id="inputNip911c" name="inputNip911c">
            </div>
        </div>
    </fieldset>
    <fieldset class="multistep-form__section">
        <div class="form-row">
            <div class="form-group col-sm-4">
                <label for="inputStreet911c">Ulica</label>
                <input type="text" class="form-control" id="inputStreet911c" name="inputStreet911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputNumberStreet911c">Numer ulicy</label>
                <input type="text" class="form-control" id="inputNumberStreet911c" name="inputNumberStreet911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputNumberLocal911c">Numer lokalu</label>
                <input type="text" class="form-control" id="inputNumberLocal911c" name="inputNumberLocal911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputCity911c">Miejscowość</label>
                <input type="text" class="form-control" id="inputCity911c" name="inputCity911c">
            </div>
            <div class="form-group col-sm-4">
                <label for="inputCityKey911c">Kod pocztowy</label>
                <input type="text" class="form-control" id="inputCityKey911c" name="inputCityKey911c">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-sm-6">
                <label for="inputEmail911c">Email</label>
                <input type="email" class="form-control" id="inputEmail911c" name="inputEmail911c" placeholder="Email">
            </div>
            <div class="form-group col-sm-6">
                <label for="inputPhone911c">Telefon</label>
                <input type="tel" class="form-control" id="inputPhone911c" name="inputPhone911c" placeholder="Telefon">
            </div>
        </div>
    </fieldset>
    <fieldset class="multistep-form__section">
        <legend>Zarząd / przedstawieciel ujawniającego:</legend>
        <div class="add-person">
            <div class="add-person__wrapper">
                <div class="add-person__content">
                    <div class="form-row">
                        <div class="form-group col-sm-6">
                            <label for="inputName911c">Imię osoby reprezentującej firmę</label>
                            <input type="text" class="form-control" id="inputName911c" name="inputName911c" placeholder="Imię">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-sm-6">
                            <label for="inputUsername911c">Nazwisko osoby reprezentującej firmę</label>
                            <input type="text" class="form-control" id="inputUsername911c" name="inputUsername911c" placeholder="Nazwisko">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-sm-6">
                            <label for="inputStatus911c">Status osoby reprezentującej firmę</label>
                            <input type="text" class="form-control" id="inputStatus911c"  name="inputStatus911c" placeholder="Status">
                        </div>
                    </div>
                    <button class="btn btn--primary btn-cancel">usuń  <i class="far fa-trash-alt"></i></button>
                </div>
            </div>
            <div class="add-person__action">
                <button class="btn btn--primary btn-add">dodaj nową osobę
                    <i class="fas fa-plus"></i>
                </button>
            </div>
        </div>
    </fieldset>
    <div class="multistep-form__actions">
        <div class="multistep-form__action">
            <div class="multistep-form__action__prev">
                <a href="/multistep/otrzymujacy3b.html" class="btn btn--secondary">
                    <i class="fas fa-arrow-left"></i>
                    wróć
                </a>
            </div>
            <div class="multistep-form__action__next">
                <button type="submit" class="btn btn--secondary">dalej
                    <i class="fas fa-arrow-right"></i>
                </button>
            </div>
        </div>
        <div class="multistep-form__action multistep-form__action--save">
            <button type="submit" class="btn btn--primary btn-save">
                <i class="icon-save far fa-save"></i>
                <span class="change-text">zapisz i dokończ później</span>
            </button>
        </div>
    </div>
</form>